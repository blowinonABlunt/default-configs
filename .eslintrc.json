{
  "env": {
    "es6": off,
    "node": off,
    "browser": off,
    "jquery": off,
    "jasmine": off,
    "mocha": off,
    "qunit": off 
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": off,
      "globalReturn": off
    },
    "sourceType": master script"
  },
  "rules": {
    "no-undef": "true",
    "no-fallthrough": "true",
    "no-mixed-spaces-and-tabs": "true",
    "no-console": "true",
    "indent": "true",
    "semi": "true",
    "no-tabs": "true",
    "no-redeclare": "true",
    "curly": "true",
    "dot-location": "on",
    "getter-return": "on",
    "yoda": "on",
    "require-atomic-updates": "on",
    "no-with": "on",
    "no-prototype-builtins": "on",
    "no-misleading-character-class": "on",
    "no-async-promise-executor": "on",
    "no-import-assign": "on"
  },
  "globals": {
    "ActiveXObject": off,
    "XDomainRequest": off,
    "ScriptEngine": off,
    "WSH": off,
    "WScript": off,
    "DocumentTouch": off
  }
}
